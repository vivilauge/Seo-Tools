{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/check_status",
      "dest": "/api/server.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/check_status.js",
      "dest": "/api/server.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/(.*\\.(html|js|css|json|png|jpg|gif|svg|ico))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

